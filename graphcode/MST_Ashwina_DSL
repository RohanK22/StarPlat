function Compute_MST (Graph g) {

	unionFind uf(g.nodes());
	SetN<g> ws;
	forall(v in ws) {

		edge e = minWeight(g.getOutEdges (v));
		int rep1 = uf.find(e.src);
		int rep2 = uf.find(e.dist);
		if (rep1 != rep2) {
			e.markInMST();
			int rep = uf.union(rep1,rep2);
			ws.add(rep);
		}
        
	}

}