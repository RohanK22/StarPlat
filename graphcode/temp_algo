function count_nodes(Graph g)
{
    int result = 0;
    propNode <int> prop;
    propNode <int> prop1;
    propNode <int> prop2;
    propNode <int> prop3;
    propNode <int> prop4;

    int x;
    int y;
    int z;
    g.attachNodeProperty(prop = y);
    x = 10 + 13;
    y = 15;
    g.attachNodeProperty(prop1 = (x<y));
    y = 15;
    z = 18;
    g.attachNodeProperty(prop2 = y);
    z = 100;
    x = 100;
    g.attachNodeProperty(prop3 = y);
}